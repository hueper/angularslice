{
  "name": "budacode-website",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "stylelint": "stylelint 'src/**/*.scss' --config stylelint-config.json --syntax scss",
    "typings-install": "./node_modules/.bin/typings install",
    "post-install": "npm run typings-install",
    "build": "ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "watch": "npm run build -- --watch",
    "start": "ENV=development ./node_modules/.bin/webpack-dev-server --config webpack.config.js --inline --colors --progress --display-error-details --display-cached --content-base src",
    "startProd": "ENV=production ./node_modules/.bin/webpack-dev-server --config webpack.config.js --inline --colors --progress --display-error-details --display-cached --content-base src"
  },
  "license": "MIT",
  "devDependencies": {
