    "awesome-typescript-loader": "^0.17.0",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "extract-loader": "^0.0.2",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "jump.js": "^0.3.2",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "node-sass": "^3.7.0",
    "open-browser-webpack-plugin": "0.0.2",
    "plop": "^1.5.0",
